<h1>Sales</h1>

<h2>Import Sales</h2>

<%= form_tag import_sales_path, multipart: true do %>
  <label for="file">File to Upload</label> <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>

<% if !@sales.nil? %>

  <% @sales.each do |sale| %>
    <%= sale.amount %>
  <% end %>
<% end %>


